{
    "manifest_version": 2,
    "name": "Okami's Tool",
    "version": "1.2.0",

    "description": "Tool crawler image",
    "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "permissions": [
      "tabs",
      "downloads", "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "icon16.png"
      ,"default_popup": "popup.html"
    }
}